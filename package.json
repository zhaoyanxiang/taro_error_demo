{
	"name": "giraffe-miniprogram",
	"version": "1.0.0",
	"private": true,
	"description": "giraffe-miniprogram",
	"templateInfo": {
		"name": "default",
		"typescript": false,
		"css": "less"
	},
	"scripts": {
		"build:weapp": "cross-env NODE_OPTIONS=--openssl-legacy-provider taro build --type weapp",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:rn": "taro build --type rn",
		"build:qq": "taro build --type qq",
		"build:jd": "taro build --type jd",
		"build:quickapp": "taro build --type quickapp",
		"dev:weapp": "cross-env NODE_OPTIONS=--openssl-legacy-provider npm run build:weapp -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn -- --watch",
		"dev:qq": "npm run build:qq -- --watch",
		"dev:jd": "npm run build:jd -- --watch",
		"dev:quickapp": "npm run build:quickapp -- --watch"
	},
	"browserslist": [
		"last 3 versions",
		"Android >= 4.1",
		"ios >= 8"
	],
	"author": "",
	"dependencies": {
		"@babel/runtime": "^7.7.7",
		"@lucky-canvas/taro": "^0.0.14",
		"@tarojs/components": "3.6.34",
		"@tarojs/plugin-framework-react": "3.6.34",
		"@tarojs/react": "3.6.34",
		"@tarojs/runtime": "3.6.34",
		"@tarojs/taro": "3.6.34",
		"@vant/area-data": "^1.1.5",
		"atob": "^2.1.2",
		"cross-env": "^7.0.3",
		"dva-core": "^2.0.4",
		"lodash": "^4.17.21",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^7.2.5",
		"redux": "^4.1.1"
	},
	"devDependencies": {
		"@babel/core": "^7.8.0",
		"@tarojs/mini-runner": "3.6.34",
		"@tarojs/webpack-runner": "3.6.34",
		"@types/react": "^17.0.2",
		"@types/webpack-env": "^1.13.6",
		"babel-preset-taro": "3.6.34",
		"eslint": "^6.8.0",
		"eslint-config-taro": "3.6.34",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"postcss": "^8.4.39",
		"stylelint": "9.3.0",
		"taro-iconfont-cli": "^3.3.0"
	}
}
